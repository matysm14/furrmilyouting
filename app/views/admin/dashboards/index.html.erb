<div class="container">
  <div class="row">
    <div class="mx-auto">
      <div class="col-md-12">

        <% @eatery.each do |eatery| %>
        <div>
        <%= link_to eatery.restaurant_name, admin_dashboard_path(eatery.id) %>
        <%= link_to eatery.user.name, user_path(eatery.user_id) %>
        <%= eatery.approved ?  "承認済み" : "未承認" %>
        </div>
        <% end %>

      </div>
    </div>
  </div>
</div>



